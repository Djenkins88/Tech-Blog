<!-- Loop over each object in the projects array -->
{{#each post as |postt| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      
      <a href="/post/{{postt.id}}">{{postt.title}}</a>
    </h2>
    <p>
    
    </p>
    <p>Created by {{post.user.name}} on {{format_date project.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{post.title}}
    </p>
  </div>
</div>
{{/each}}
